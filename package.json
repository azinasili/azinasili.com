{
  "name": "azinasili.com",
  "description": "Personal website and portfolio",
  "version": "2.3.2",
  "authors": "Azin Asili <hello@azinasili.com>",
  "homepage": "http://azinasili.com/",
  "private": true,
  "type": "module",
  "scripts": {
    "analyze": "npx next experimental-analyze",
    "build": "next build",
    "dev": "next dev",
    "eslint": "eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint:fix-file": "npm run eslint:fix -- --file",
    "eslint:staged": "eslint --fix",
    "format": "npm run prettier:fix && npm run eslint:fix && npm run stylelint:fix",
    "format:check": "npm run prettier && npm run eslint && npm run stylelint",
    "lint": "eslint",
    "precommit": "lint-staged",
    "prepare": "husky",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prettier:fix-file": "prettier --write",
    "prettier:staged": "npm run prettier:fix-file",
    "start": "next start",
    "stylelint": "stylelint \"**/*.css\" --config stylelint.config.ts",
    "stylelint:fix": "npm run stylelint -- --fix",
    "stylelint:staged": "npm run stylelint:fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "modern-normalize": "3.0.1",
    "next": "16.1.6",
    "react": "19.2.4",
    "react-dom": "19.2.4"
  },
  "devDependencies": {
    "@types/node": "25.1.0",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "10.1.8",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "stylelint": "17.1.0",
    "stylelint-config-standard": "40.0.0",
    "stylelint-prettier": "5.0.3",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0"
  }
}
